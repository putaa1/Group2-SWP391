        <servlet-name>deleteOrder</servlet-name>
        <servlet-class>controller.Order.deleteOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchOrder</servlet-name>
        <servlet-class>controller.Order.searchOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateOrder</servlet-name>
        <servlet-class>controller.Order.updateOrder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newsList</servlet-name>
        <url-pattern>/newsList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listCate</servlet-name>
        <url-pattern>/listCate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteCate</servlet-name>
        <url-pattern>/deleteCate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateCate</servlet-name>
        <url-pattern>/updateCate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addCate</servlet-name>
        <url-pattern>/addCate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchCate</servlet-name>
        <url-pattern>/searchCate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addOrder</servlet-name>
        <url-pattern>/addOrder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listOrder</servlet-name>
        <url-pattern>/listOrder</url-pattern>
    </servlet-mapping>
